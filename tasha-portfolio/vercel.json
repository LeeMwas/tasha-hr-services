{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      }
    ],
    "routes": [
      {
        "src": "/",
        "dest": "/"
      },
      {
        "src": "/services",
        "dest": "/"
      },
      {
        "src": "/about",
        "dest": "/"
      },
      {
        "src": "/contact",
        "dest": "/"
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
  }